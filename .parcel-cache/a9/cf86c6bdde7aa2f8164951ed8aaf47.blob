/* http://meyerweb.com/eric/tools/css/reset/ 
   v2.0 | 20110126
   License: none (public domain)
*/

html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	font: inherit;
	vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
body {
  font-family: OpenSans, AdobeInvisFont, MyriadPro-Regular, sans-serif;
}
.container {
  max-width: 1170px;
  margin: 0 auto;
}
.btn {
  background-color: #3172B9;
  border: none;
  padding: 15px 35px;
  text-transform: uppercase;
  color: #fff;
  border-radius: 5px;
  font-size: 15px;
  cursor: pointer;
  font-family: inherit;
  transition: background-color 0.2s ease;
}
.btn:hover {
  background-color: #397fcb;
  transition: background-color 0.2s ease;
}
.header {
  border-bottom: 2px solid #e4e4e4;
  z-index: 10;
}
.header__container {
  height: 110px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header__logo {
  margin-right: 90px;
}
.header__logo-img {
  width: 116px;
  height: 45px;
}
.header__nav {
  height: 100%;
  display: flex;
  align-items: center;
}
.header__menu {
  display: flex;
  height: 100%;
}
.header__menu-item {
  display: block;
  cursor: pointer;
  border-bottom: 5px solid transparent;
  transition: all 0.2s ease;
}
.header__menu-item:hover {
  border-bottom: 5px solid #3172B9;
  color: #3172B9;
  transition: all 0.2s ease;
}
.header__menu-item--dropdown {
  position: relative;
}
.header__menu-item--dropdown::after {
  content: url("arrow-down.HASH_REF_b3c28f23e7fe5a3b643f4237f60c8f63.png");
  position: absolute;
  top: 50%;
  right: -20px;
  transform: translateY(-50%);
}
.header__menu-link {
  display: flex;
  align-items: center;
  height: 100%;
  font-size: 15px;
  font-weight: 600;
}
.header__menu-item:not(:last-child) {
  margin-right: 60px;
}
.hero {
  background: url("office-organized.HASH_REF_623083f6e9b0b5bfc5b6e7a6f1a67bb6.jpg") no-repeat;
  background-size: cover;
  min-height: 550px;
}
.hero__container {
  padding-top: 125px;
}
.hero__title {
  color: #1c1c1c;
  font-size: 40px;
  font-weight: 300;
  max-width: 510px;
  margin-bottom: 30px;
  line-height: 120%;
}
.hero__text {
  margin-bottom: 30px;
  max-width: 470px;
  line-height: 140%;
  color: #787878;
}
.btn__wrapper {
  width: 250px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.hero__btn {
  width: 250px;
}
.hero__hint {
  margin-top: 20px;
  position: relative;
  text-transform: uppercase;
  color: #787878;
  font-size: 9px;
  margin-left: 22px;
}
.hero__hint::before {
  content: url("shield.HASH_REF_ddbab1db19413c55f8b38908121bfa22.png");
  width: 15px;
  height: 18px;
  position: absolute;
  top: -5px;
  left: -22px;
}
.services__container {
  padding-top: 225px;
  padding-bottom: 200px;
}
