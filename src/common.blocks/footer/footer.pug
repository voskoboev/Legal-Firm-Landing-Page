- const footerTitlesArr = ['About Us', 'Contact Us', 'Privacy Policy', 'Terms Condition']
- const picsArr = [picTwitter, picLinkedin, picEmail]

footer.footer 
  .container.footer__container 
    .footer__logo
      a.footer__logo-link(href="#") 
        img.footer__logo-img(src="./images/logo.png " alt="logo")
    p.footer_copyright Copyright &copy; 2021. All Right Reserved
    nav.footer__nav
      ul.footer__menu
        each val in footerTitlesArr 
          li.footer_menu-item
            a.footer__menu-link(href="#")= val
    ul.footer__socials
      each val in picsArr 
        li.footer__socials-item
          a.footer__socials-link(href="#" target="_blank" rel="noopener")!= val
